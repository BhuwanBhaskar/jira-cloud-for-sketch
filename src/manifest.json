{
	"name": "JIRA Sketch plugin",
	"author": "Atlassian",
	"email": "tim@atlassian.com",
	"appcast": "https://s3-us-west-2.amazonaws.com/atlassian-sketch-plugin/appcast.xml",
	"compatibleVersion": 3,
	"bundleVersion": 1,
	"commands": [
		{
			"name": "Connect to JIRA Cloud",
			"identifier": "connect",
			"script": "./commands/connect.js"
		},
		{
			"name": "View Issues",
			"identifier": "issues",
			"script": "./commands/issues.js",
			"shortcut": "cmd alt j"
		},
		{
			"name": "Feedback...",
			"identifier": "feedback",
			"script": "./commands/feedback.js"
		},
		{
			"name": "StartUp",
			"identifier": "startup",
			"script": "./commands/startup.js",
			"handlers": {
				"actions": {
					"Startup": "onRun"
				}
			}
		},
		{
			"name": "Test Action",
			"identifier": "test",
			"script": "./commands/test.js",
			"shortcut": "cmd alt b"
		}
	],
	"menu": {
		"title": "JIRA",
		"items": [
			"connect",
			"issues",
			"feedback"
		]
	}
}