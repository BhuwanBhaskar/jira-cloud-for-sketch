{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "Connect to JIRA Cloud",
      "identifier": "connect",
      "script": "connect.js",
      "shortcut": "cmd alt k"
    },
    {
      "name": "Recent issues",
      "identifier": "issues",
      "script": "issues.js",
      "shortcut": "cmd alt j"
    },
    {
      "name": "Export to JIRA",
      "identifier": "export",
      "script": "export.js",
      "shortcut": "cmd alt ;"
    },
    {
      "name": "Test action",
      "identifier": "test",
      "script": "test.js",
      "shortcut": "cmd alt b"
    }
  ],
  "menu": {
    "title": "JIRA",
    "items": [
      "connect",
      "issues",
      "export",
      "test"
    ]
  },
  "version": "0.0.1",
  "description": "A Sketch plugin for interacting with Atlassian JIRA",
  "homepage": "https://bitbucket.org/tpettersen/jira-sketch-plugin#readme",
  "name": "jira-for-sketch",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml",
  "author": "Tim Pettersen",
  "email": "tim@atlassian.com"
}